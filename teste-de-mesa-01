int a = 10;
int b = 20;
int c = 0; 


Console.WriteLine($"O valor de a é: {a}");
Console.WriteLine($"O valor de b é: {b}");
Console.WriteLine("O problema apresentado é c=a+b / 2 e em seguida c-40");
c = (a + b) / 2;
Console.WriteLine($"Portanto valor de c=a+b / 2 é: {c}");
c = c - 40;
Console.WriteLine($"Subtraindo 40 o valor final de c é: {c}");

int[] v = new int[3] { a, b, c };



